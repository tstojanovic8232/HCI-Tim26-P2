        <TextBox  Name="email" HorizontalAlignment="Left" Height="23" Margin="305,178,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="213" RenderTransformOrigin="0.497,2.851"/>
        <TextBox  Name="datum_rodjenja" HorizontalAlignment="Left" Height="23" Margin="305,228,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="213" RenderTransformOrigin="0.497,2.851"/>
