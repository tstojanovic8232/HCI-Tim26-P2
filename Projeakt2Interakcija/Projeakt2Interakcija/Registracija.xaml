        mc:Ignorable="d"
        Title="Registracija" Height="450" Width="800">
    <Grid>
        <TextBox Name="ime" HorizontalAlignment="Left" Height="23" Margin="305,77,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="213"/>
